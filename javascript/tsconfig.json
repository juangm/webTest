{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "pretty": true,
    "noEmit": true,
    "strict": false,
    "baseUrl": ".",
    "paths": {},
    "skipLibCheck": true,
    "typeRoots": [
      "./node_modules/@types",
      "./node_modules"
    ],
    "types": [
      "node",
      "puppeteer"
    ]
  },
  "exclude": [
    "node_modules"
  ],
  "compileOnSave": false
}
